enum TripStatus {
    REQUESTED, ACTIVE, CANCELLED, COMPLETED
}

entity Trip {
	driverId Integer,
	riderId Integer,
	tripStatus	TripStatus,
	duration	Float,
	distance	Float,
    id Integer
}




// Set pagination options
paginate Trip with infinite-scroll

dto * with mapstruct

// Set service options to all except few
service all with serviceClass
